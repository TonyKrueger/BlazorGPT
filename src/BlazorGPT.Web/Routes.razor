<ViewTransitionRouter AppAssembly="@typeof(Program).Assembly" AdditionalAssemblies="@AvailableAssemblies" PreferExactMatches="@true">
    <Found Context="routeData">
        <AuthorizeRouteView RouteData="@routeData" DefaultLayout="@typeof(BlazorGptLayout)">
            <NotAuthorized>
                <RedirectToLogin/>
            </NotAuthorized>
        </AuthorizeRouteView>
        <FocusOnNavigate RouteData="@routeData" Selector="[autofocus]"/>
    </Found>
    <NotFound></NotFound>
</ViewTransitionRouter>